<h1>Livros disponíveis:</h1>
<div  class="book-container" >
    <a href="/">Adicionar livro</a>
    {{#each books}}
        <div>
            #{{this.id}} - <span class="bold" >
                <a href="/books/{{this.id}}">
                    {{this.title}}
                </a>
                </span> - {{this.page}} páginas
        </div>
    {{/each}}
</div>